{
  "name": "playwrightautomation",
  "version": "1.0.0",
  "scripts": {
    "test": "npx playwright test",
    "build": "npx tsc",
    "package": "npm run build && pkg . --targets node18-win-x64 --outputs my-test.exe"
  },
  "bin": "dist/run-my-test.js",
  "pkg": {
    "assets": [
      "dist/**"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.2",
    "pkg": "^5.8.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "playwright": "^1.56.1"
  }
}
